
def prime(num):
    for i in range(2, num):
        if num < 2:
            return False
        else:
            if num%2==0:
                return False
            else:
                return True
    return True

num = int(input("enter number"))
if prime(num):
    print(num,"is prime number")
else:
    print(num,"is not prime number")
    
    

          
        








